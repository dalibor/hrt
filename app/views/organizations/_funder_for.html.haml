%table.standard
  %thead
    %tr
      %th Organization
      %th Project
  %tbody
    - org.out_flows.each do |of|
      - response = organization_response(of.organization, current_request)
      - proj_path = edit_project_path(of.project)
      %tr
        %td= link_to of.organization.name, projects_path
        %td= link_to of.project.name, proj_path
