- title("Import: Review & Save")

- content_for :sub_nav do
  = render :partial => 'shared/sub_nav_projects'

%h1.main_heading Import: Review & Save

%p Please review each of your uploaded projects & activities, and press Save for each project you want to import.

/ = render 'getting_started_bulk'

- @projects.each_with_index do |project, index|
  = render 'projects/bulk_review', :project => project, :response => @response

- @other_costs.each_with_index do |other_cost, index|
  = render 'projects/bulk_review_other_cost', :other_cost => other_cost, :response => @response

%br

= link_to "Return to Projects & Activities", projects_path, :class => 'js_finish cancel'
